    "[data-store-bullets]": {
      "display": "flex",
      "listStyleType": "none",
      "paddingY": "10px",
      "paddingX": 0,
      "marginTop": "0.5rem",
      "marginBottom": "0.5rem",
      "position": "relative",
      "justifyContent": "center",
      "alignItems": "center",
      "margin": 0,
      "bottom": 0,
      "left": 0,
      "right": 0
    },
    "[data-store-button]": {
      "bg": "#a8a8a8",
      "cursor": "pointer",
      "display": "inline-block",
      "borderRadius": "100%",
      "margin": 12,
