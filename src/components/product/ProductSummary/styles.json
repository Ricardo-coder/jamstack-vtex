{
  "link": {
    "maxWidth": "300px",
    "minWidth": "150px",
    "textDecoration": "none",
    "color": "text",
    "flexGrow": 1,
    "display": "inline-grid",
    "justifySelf": "center",
    "width": "100%"
  },
  "title": {
    "fontWeight": 400,
    "fontSize": "14px",
    "height": "75px",
    "color": "#071923",
    "marginTop": "1rem"
  },
  "imageContainer": {
    "width": "100%",
    "aspectRatio": "1"
  },
  "image": {
    "height": "100%",
    "maxWidth": "100%",
    "aspectRatio": "1",
    "objectFit": "contain"
  }
}
