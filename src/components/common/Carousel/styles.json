{
  "position": "relative",
  "image": {
    "height": ["540px", "614px"],
    "zIndex": -2
  },
  "arrow": {
    "left": {
      "position": "absolute",
      "top": "50%",
      "left": 0,
      "zIndex": 1,
      "bg": "transparent",
      "borderColor": "transparent",
      "cursor": "pointer",
      "&:hover": {
        "opacity": "0.5"
      },
      "svg": {
        "color": "text"
      }
    },
    "right": {
      "position": "absolute",
      "top": "50%",
      "right": 0,
      "zIndex": 1,
      "bg": "transparent",
      "borderColor": "transparent",
      "cursor": "pointer",
      "&:hover": {
        "opacity": "0.5"
      },
      "svg": {
        "color": "text"
      }
    }
  },
  "paginationDots": {
